ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Whodas 2 0">
					description = <"World Health Organization Disability Assessment Schedule 2.0 enquires about difficulties or disabilities a patient may experience due to health/mental health issues in adults over 18 yrs">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Any event">
					description = <"*">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"D1.1 S6: Concentrating on doing something for ten minutes? ">
					description = <"Understanding and communicating">
				>
				["at0005"] = <
					text = <"D1.2: Remembering to do important things?">
					description = <"Understanding and communicating">
				>
				["at0006"] = <
					text = <"D1.3: Analysing and finding solutions to problems in day-to-day life?">
					description = <"Understanding and communicating">
				>
				["at0007"] = <
					text = <"D1.4 S3: Learning a new task, for example, learning how to get to a new place?">
					description = <"Understanding and communicating">
				>
				["at0008"] = <
					text = <"D1.5: Generally understanding what people say?">
					description = <"Understanding and communicating">
				>
				["at0009"] = <
					text = <"D1.6: Starting and maintaining a conversation?">
					description = <"Understanding and communicating">
				>
				["at0010"] = <
					text = <"D2.1 S1: Standing for long periods such as 30 minutes?">
					description = <"Getting around">
				>
				["at0011"] = <
					text = <"D2.2: Standing up from sitting down?">
					description = <"Getting around">
				>
				["at0012"] = <
					text = <"D2.3: Moving around inside your home?">
					description = <"Getting around">
				>
				["at0013"] = <
					text = <"D2.4: Getting out of your home?">
					description = <"Getting around">
				>
				["at0014"] = <
					text = <"D2.5 S7: Walking a long distance such as a kilometre [or equivalent]?">
					description = <"Getting around">
				>
				["at0015"] = <
					text = <"D3.1 S8: Washing your whole body?">
					description = <"Self-care">
				>
				["at0016"] = <
					text = <"D3.2 S9: Getting dressed?">
					description = <"Self-care">
				>
				["at0017"] = <
					text = <"D3.3: Eating?">
					description = <"Self-care">
				>
				["at0018"] = <
					text = <"D3.4: Staying by yourself for a few days?">
					description = <"Self-care">
				>
				["at0019"] = <
					text = <"D5.3: Getting all the household work done that you needed to do?">
					description = <"Life-activities household">
				>
				["at0020"] = <
					text = <"D5.4: Getting your household work done as quickly as needed?">
					description = <"Life-activities household">
				>
				["at0021"] = <
					text = <"D5.5 S12: Your day-to-day work/school?">
					description = <"Life activities—School or Work">
				>
				["at0022"] = <
					text = <"D5.7: Getting all the work done that you need to do?">
					description = <"Life activities—School or Work">
				>
				["at0023"] = <
					text = <"D5.8: Getting your work done as quickly as needed?">
					description = <"Life activities—School or Work">
				>
				["at0024"] = <
					text = <"D6.1 S4: How much of a problem did you have in joining in community activities (for example, festivities, religious or other activities) in the same way as anyone else can?">
					description = <"Participation in society">
				>
				["at0025"] = <
					text = <"D6.2: How much of a problem did you have because of barriers or hindrances in the world around you?">
					description = <"Participation in society">
				>
				["at0026"] = <
					text = <"D5.6: Doing your most important work/school tasks well?">
					description = <"Life activities—School or Work">
				>
				["at0027"] = <
					text = <"D4.4: Making new friends?">
					description = <"Getting along with people">
				>
				["at0028"] = <
					text = <"D5.2: Doing most important household tasks well?">
					description = <"Life-activities household">
				>
				["at0029"] = <
					text = <"D5.1 S2: Taking care of your household responsibilities?">
					description = <"Life-activities household">
				>
				["at0030"] = <
					text = <"D4.5: Sexual activities?">
					description = <"Getting along with people">
				>
				["at0031"] = <
					text = <"D4.3: Getting along with people who are close to you?">
					description = <"Getting along with people">
				>
				["at0032"] = <
					text = <"D4.2 S11: Maintaining a friendship?">
					description = <"Getting along with people">
				>
				["at0033"] = <
					text = <"D4.1 S10: Dealing with people you do not know?">
					description = <"Getting along with people">
				>
				["at0034"] = <
					text = <"D6.3: How much of a problem did you have living with dignity because of the attitudes and actions of others?">
					description = <"Participation in society">
				>
				["at0035"] = <
					text = <"D6.4 How much time did you spend on your health condition, or its consequences?">
					description = <"Participation in society">
				>
				["at0036"] = <
					text = <"D6.5 S5: How much have you been emotionally affected by your health condition?">
					description = <"Participation in society">
				>
				["at0037"] = <
					text = <"D6.6: How much has your health been a drain on the financial resources of you or your family?">
					description = <"Participation in society">
				>
				["at0038"] = <
					text = <"D6.7: How much of a problem did your family have because of your health problems?">
					description = <"Participation in society">
				>
				["at0039"] = <
					text = <"D6.8: How much of a problem did you have in doing things by yourself for relaxation or  pleasure?">
					description = <"Participation in society">
				>
				["at0040"] = <
					text = <"12 item raw score">
					description = <"Simple raw score as a percentage of the 12 most important questions creating a quick score to assess overall functioning. explains 81% of the variance of the 36 item version.  These 12 Qs are indicated on the ordinals and labelled with S lettering and number beside the D lettering. Sum all 12 item scores then divide by 48">
				>
				["at0041"] = <
					text = <"12 - 24 ques score Hybrid version">
					description = <"The additional 24 items if any of the previous first 5 of the 12 questions in the 12 version were positive then further investigation is  needed. The last 5 of the 12 version is answered and the remaining 24 items answered after that. If from teh 12 version Q3/6 were positive: ">
				>
				["at0042"] = <
					text = <"36 item raw score">
					description = <"All 36 items summed for the raw scores then divided by 144 as a percentage.">
				>
				["at0043"] = <
					text = <"Comment">
					description = <"Comments regarding the score">
				>
				["at0044"] = <
					text = <"D1 Understanding and communicating domain score">
					description = <"Understanding and communicating complex domain score as a percentage of the total score within D1 divided by 24">
				>
				["at0045"] = <
					text = <"D2 Getting around domain score">
					description = <"Getting around complex domain score as a percentage of the total score within D2 divided by 20">
				>
				["at0046"] = <
					text = <"D3 Self-care domain score">
					description = <"Self-care complex domain score as a percentage of the total score within D3 divided by 16">
				>
				["at0047"] = <
					text = <"36 item complex score">
					description = <"Complex score based n IRT - item-response theory which takes into account the weighted scoring per type of response and is a percentage (calculated by taking the average percentage score over the 6 domains)">
				>
				["at0048"] = <
					text = <"D6 Participation in society domain score">
					description = <"Participation in society complex domain score as a percentage of the total score within D6 divided by 32">
				>
				["at0049"] = <
					text = <"D5 Life activities domain score">
					description = <"Life activities complex domain score as a percentage of the total score within D5 divided by 32">
				>
				["at0050"] = <

					text = <"*Any event(en)">
					description = <"**(en)">
				>
				["at0003"] = <
					text = <"*Tree(en)">
					description = <"*@ internal @(en)">
				>
				["at0004"] = <
					text = <"D1.1 S6: Att koncentrera dig på att göra något under tio minuter?">
					description = <"Kognition">
				>
				["at0005"] = <
					text = <"D1.2: Komma ihåg att göra viktiga saker?">
					description = <"Kognition">
				>
				["at0006"] = <
					text = <"D1.3: Analysera och finna lösningar på problem i det dagliga livet?">
					description = <"Kognition">
				>
				["at0007"] = <
					text = <"D1.4 S3: Att lära dig en ny uppgift, t.ex. hur man tar sig till en ny plats?">
					description = <"Kognition">
				>
				["at0008"] = <
					text = <"D1.5: För det mesta förstå vad människor säger?">
					description = <"Kognition">
				>
				["at0009"] = <
					text = <"D1.6: Påbörja och hålla igång ett samtal?">
					description = <"Kognition">
				>
				["at0010"] = <
					text = <"D2.1 S1: Att stå under längre perioder såsom 30 minuter?">
					description = <"Förflyttning">
				>
				["at0011"] = <
					text = <"D2.2: Resa dig från sittande ställning?">
					description = <"Förflyttning">
				>
				["at0012"] = <
					text = <"D2.3: Röra dig i ditt hem?">
					description = <"Förflyttning">
				>
				["at0013"] = <
					text = <"D2.4: Ta dig ut från ditt hem?">
					description = <"Förflyttning">
				>
				["at0014"] = <
					text = <"D2.5 S7: Att gå en längre sträcka såsom en kilometer?">
					description = <"Förflyttning">
